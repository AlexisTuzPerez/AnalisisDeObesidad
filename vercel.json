{
  "version": 2,
  "builds": [
    {
      "src": "ProyectoFinal.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/ProyectoFinal.html"
    }
  ],
  "settings": {
    "buildCommand": "echo 'Building static HTML site'",
    "outputDirectory": ""
  }
}
